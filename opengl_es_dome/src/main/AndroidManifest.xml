<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunongtong.yunong.opengl_es_dome" >

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OpenGLES20Activity"
            android:label="@string/title_activity_open_gles20" />
        <activity
            android:name=".ThemeTestActivity"
            android:label="@string/title_activity_actionbar" />
        <activity
            android:name=".ColorGameActivity"
            android:label="@string/title_activity_color_game" >
        </activity>
        <activity android:name="com.example.zhy_baseadapterhelper.MainActivity" />
        <activity android:name="com.example.zhy_baseadapterhelper.SimpleTestActivity" />
        <activity android:name="com.example.zhy_baseadapterhelper.ChatActivity" />
        <activity
            android:name="com.event_bus.EventBusMainActivity"
            android:label="@string/title_activity_event_bus_main" >
        </activity>
    </application>

</manifest>
